{
  "name": "prompt-library",
  "description": "A starter template to make browser extension using WXT",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "git-remove": "bun run ./scripts/git-helper.js remove-git",
    "git-change": "bun run ./scripts/git-helper.js change-repo",
    "git-show": "bun run ./scripts/git-helper.js show-git",
    "git-add": "bun run ./scripts/git-helper.js add-git",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.23.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.5",
    "@types/lodash": "^4.17.17",
    "antd": "^5.25.4",
    "arrive": "^2.5.2",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "color": "^5.0.2",
    "framer-motion": "^12.23.24",
    "lodash": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.8",
    "webext-bridge": "^6.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/arrive": "^2.4.4",
    "@types/chrome": "^0.1.12",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-react": "^1.1.5",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "wxt": "^0.20.11"
  },
  "overrides": {
    "@wxt-dev/browser": "^0.1.4"
  }
}
